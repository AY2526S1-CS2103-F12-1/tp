@startuml
!include style.puml
skinparam arrowThickness 1.1
skinparam arrowColor MODEL_COLOR
skinparam classBackgroundColor MODEL_COLOR

Package Model as ModelPackage <<Rectangle>>{
Class "<<interface>>\nReadOnlyHenri" as ReadOnlyHenri
Class "<<interface>>\nReadOnlyUserPrefs" as ReadOnlyUserPrefs
Class "<<interface>>\nModel" as Model
Class Henri
Class ModelManager
Class UserPrefs

Class UniquePersonList
Class UniqueTeamList
Class Team
Class TeamName
Class Members
Class LeaderId
Class Id
Class Person
Class Address
Class Email
Class Name
Class Phone
Class Tag
Class GitHubUsername
Class TeamId

Class I #FFFFFF
}

Class HiddenOutside #FFFFFF
HiddenOutside ..> Model

Henri .up.|> ReadOnlyHenri

ModelManager .up.|> Model
Model .right.> ReadOnlyUserPrefs
Model .left.> ReadOnlyHenri
ModelManager -left-> "1" Henri
ModelManager -right-> "1" UserPrefs
UserPrefs .up.|> ReadOnlyUserPrefs

Henri *--> "1" UniquePersonList
Henri *--> "1" UniqueTeamList

UniquePersonList --> "~* all" Person
Person *--> "1" Name
Person *--> "1" Phone
Person *--> "1" Email
Person *--> "1" Address
Person *--> "0..1" GitHubUsername
Person *--> "*" Tag
Person --> "*" TeamId

UniqueTeamList --> "~* all" Team
Team *--> Id
Team *--> TeamName
Team *--> LeaderId
Team *--> "*" Members
Team -up-> "~* SubTeams" Team
Team -right-> "1 ParentTeam" Team


Person -[hidden]up--> I
UniquePersonList -[hidden]right-> I

Name -[hidden]right-> Phone
Phone -[hidden]right-> Address
Address -[hidden]right-> GitHubUsername
GitHubUsername -[hidden]right-> Email

ModelManager --> "~* filtered" Person
@enduml
